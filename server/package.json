{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "engines": {
    "node": "12.16.1",
    "npm": "6.14.6"
  },
  "scripts": {
    "client-install": "cd ../client/ && npm install",
    "build": "cd ../client && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm start --prefix ../client",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cloudinary": "^1.22.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
